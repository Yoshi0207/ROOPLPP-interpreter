class Test
  method test(int x)
  switch x
    case 1: x += 2  rcase 7
    case 2: x += 4  rcase 6
            break
    case 3: print("a") rcase 3
    case 4: print("b") rcase 4
    case 5: print("c") rcase 5
            break
  rswitch x

class Program
int x
Test t
  method main()
  x ^= 1
  new Test t
  show(x)
  print("\n順実行\n")
  call t::test(x)
  show(x)
  print("\n逆実行\n")
  uncall t::test(x)
  show(x)
  print("\n")
