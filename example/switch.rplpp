class Test
  method test(int x)
  switch x
    case 1: x += 2 rcase 7
    case 2: x += 4 rcase 6
            break
    case 3: print("a") rcase 3
    case 4: print("b") rcase 4
    case 5: print("c") rcase 5
            break
  rswitch x

class Program
int x
Test t
  method main()
  x ^= 3
  new Test t
  call t::test(x)
  print("\n")
  uncall t::test(x)
  print("\n")